generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL_MATINDOK")
}

model tbalarms {
  id        Int     @id
  group     String? @db.Text
  tag       String? @db.Text
  condition String? @db.Text
  constant  String? @db.Text
  text      String? @db.Text
}

model tbtags {
  id   Int     @id
  tag  String? @db.Text
  desc String? @db.Text
}

model realtime {
  id     Int     @id
  tname  String? @db.VarChar(50)
  tvalue String? @db.VarChar(50)
}
