generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model realtime {
  id     Int     @id
  tname  String  @db.VarChar(45)
  tvalue String? @db.VarChar(45)
}

model alg_24 {
  id        BigInt  @id @default(autoincrement())
  timestamp BigInt
  alarmid   Int
  status    String? @db.VarChar(45)
}
